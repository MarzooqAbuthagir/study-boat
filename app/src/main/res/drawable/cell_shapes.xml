<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffff" />
        </shape>
    </item>
    <item android:left="0.5dp"
        android:right="0.5dp"
        android:bottom="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#4bb2f7" />
        </shape>
    </item>
</layer-list>


